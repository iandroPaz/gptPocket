{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "types": ["node"]
  },
  "typeRoots": ["./types"],
  "ts-node": {
    "files": true
  },
  "include": ["src/**/*.ts", "env.ts"],
  "exclude": [
    "node_modules"
  ]
}
